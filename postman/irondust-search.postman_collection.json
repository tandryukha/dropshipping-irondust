        "method": "POST",
        "header": [ {"key": "x-admin-key", "value": "{{adminKey}}"} ],
        "url": { "raw": "{{baseUrl}}/feature-flags/ai_search?enabled=false", "host": ["{{baseUrl}}"], "path": ["feature-flags","ai_search"], "query": [{"key":"enabled","value":"false"}] }
      }
    },
